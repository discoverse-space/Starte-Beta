{
  "name": "starte",
  "version": "v0.4.1",
  "main": "main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "packager": "electron-builder build"
  },
  "author": "zestela.co",
  "devDependencies": {
    "electron": "^21.3.3",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "productName": "观星记 Starte",
    "appId": "com.zestela.starte",
    "copyright": "Zestela",
    "directories": {
      "output": "build"
    },
    "nsis": {
      "oneClick": false,
      "language": "2052",
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "installerIcon": "./src/icons/dock.ico",
      "uninstallerIcon": "./src/icons/dock.ico",
      "installerHeaderIcon": "./src/icons/dock.ico",
      "shortcutName": "观星记 Starte"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://api.zestela.co/"
      }
    ],
    "win": {
      "icon": "./src/icons/dock.ico",
      "target": [
        {
          "target": "nsis"
        }
      ]
    }
  },
  "dependencies": {
    "axios": "^1.2.2",
    "form-data": "^4.0.0",
    "node-machine-id": "^1.1.12",
    "v8-compile-cache": "^2.3.0"
  }
}
